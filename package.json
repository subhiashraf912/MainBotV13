{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "next build",
    "build": "npm run build:next && npm run build:server",
    "start": "NODE_ENV=production node dist/server/app.js",
    "copy-canvacord-images": "copyfiles --error --up 2 src/server/client/utils/Modules/@azecord/assets/data/images/*.* dist/server",
    "copy-canvacord-fonts": "copyfiles --error --up 2 src/server/client/utils/Modules/@azecord/assets/data/fonts/*.* dist/server",
    "copy-aze-canvas-fonts": "copyfiles --error --up 2 src/server/client/utils/Modules/AzeCord/aze-canvas/assets/fonts/*.* dist/server",
    "copy-aze-canvas-images": "copyfiles --error --up 2 src/server/client/utils/Modules/AzeCord/aze-canvas/assets/img/*.* dist/server",
    "postbuild": "npm run copy-canvacord-images && npm run copy-canvacord-fonts && npm run copy-aze-canvas-images && npm run copy-aze-canvas-fonts",
    "lint": "eslint src",
    "prettier": "prettier --write src/**/*",
    "lint:fix": "eslint src --fix"
  
  },
  "dependencies": {
    "@distube/soundcloud": "^1.0.0",
    "@distube/spotify": "^1.0.0",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@types/ms": "^0.7.31",
    "@vitalets/google-translate-api": "^7.0.0",
    "aki-api": "^6.0.8",
    "canvas": "^2.8.0",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^1.4.0",
    "crypto-js": "^4.1.1",
    "discord-giveaways": "^5.0.1",
    "discord-ytdl-core": "^5.0.4",
    "discord.js": "^13.3.1",
    "distube": "^3.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "fluent-ffmpeg": "^2.1.2",
    "genius-lyrics": "^4.3.4",
    "genshin-db": "^3.12.0",
    "gifencoder": "^2.0.1",
    "js-cookie": "^3.0.1",
    "mal-scraper": "^2.11.3",
    "mathjs": "^10.0.0",
    "moment": "^2.29.1",
    "moment-duration-format": "^2.3.2",
    "mongoose": "^6.0.13",
    "ms": "^2.1.3",
    "next": "12.0.1",
    "node-canvas-with-twemoji-and-discord-emoji": "^1.2.1",
    "node-kitsu": "^1.1.1",
    "node-url-shortener": "^2.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "request": "^2.88.2",
    "serve-favicon": "^2.5.0",
    "smart-botto": "^1.1.1",
    "string-progressbar": "^1.0.4",
    "wetransfert": "^2.3.4",
    "ytdl-core": "^4.9.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-session": "^2.0.43",
    "@types/crypto-js": "^4.0.2",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/gifencoder": "^2.0.1",
    "@types/js-cookie": "^3.0.1",
    "@types/moment-duration-format": "^2.2.3",
    "@types/node": "16.11.6",
    "@types/react": "17.0.33",
    "@types/request": "^2.48.7",
    "@types/serve-favicon": "^2.5.3",
    "copyfiles": "^2.4.1",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.1",
    "typescript": "4.4.4"
  }
}
